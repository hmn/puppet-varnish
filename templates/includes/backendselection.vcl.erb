# <%= @title %>
if (<%= @condition -%>) {
<%- if @movedto -%>
  return (synth(750, "<%= @movedto -%>" + req.url));
<%- else -%>
  set req.backend_hint = <%= @director -%>;
  <%- if @rewrite -%>
  set req.http.x-host = <%= @rewrite -%>;
  <%- end -%>
  <%- if @newurl -%>
  set req.url = <%= @newurl -%>;
  <%- end -%>
  <%- if @return -%>
  return (<%= @return -%>);
  <%- end -%>
<%- end -%>
}
